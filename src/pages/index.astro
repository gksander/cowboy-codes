import _portrait from "../assets/portrait-1.png";
import { Homepage } from "../components/homepage/homepage";
import { getOptimizedImageSrc } from "../utils/getOptimizedImageSrc";

const portrait = await getOptimizedImageSrc({ src: _portrait, width: 450 });
---

<DefaultLayout>
  <Homepage portrait={portrait} />
</DefaultLayout>
